{
    "name": "anthony-brindley/design-pattern-implementor",
    "description": "A Laravel package designed to speed up functionality implementation using design patterns",
    "autoload": {
        "psr-4": {
            "Anthonybrindley\\DesignPatternImplementor\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Anthony Brindley",
            "email": "anthony@anthonybrindley.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.16",
        "laravel/prompts": "^0.3.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anthonybrindley\\DesignPatternImplementor\\Providers\\DesignPatternImplementorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}